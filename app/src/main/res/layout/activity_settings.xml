<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/general_padding_10"
    android:orientation="vertical"
    tools:context=".Setting.SettingsActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


    <TextView
        android:id="@+id/changePass"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:hint="@string/change_pass"
        android:imeOptions="actionDone"
        android:padding="@dimen/general_padding_10"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textSize="@dimen/font_18"
        android:text="@string/change_pass"/>
    <TextView
        android:id="@+id/Favadd_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:hint="@string/address"
        android:imeOptions="actionDone"
        android:padding="@dimen/general_padding_10"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textSize="@dimen/font_18"
        tools:text="@string/address"/>
        <TextView
            android:id="@+id/profile_setting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:hint="@string/profile"
            android:imeOptions="actionDone"
            android:padding="@dimen/general_padding_10"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textSize="@dimen/font_18"
            tools:text="@string/profile"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/contain_setting">

    </LinearLayout>
</FrameLayout>